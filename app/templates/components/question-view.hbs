<div class="container row">
	{{#each model as |question|}}
		<div class="col-xs-6">
			<h3>{{question.title}}</h3>by {{question.author}}
			<div>
				<p>{{question.query}}</p>
				<button class="btn btn-default">{{#link-to 'question' question.id}}Open{{/link-to}}</button>
			</div>
		</div>	
	{{/each}}
</div>